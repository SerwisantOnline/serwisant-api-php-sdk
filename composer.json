{
  "name": "serwisant/serwisant-api",
  "version": "3.2.2",
  "description": "API client for https://serwisant.online service",
  "type": "library",
  "homepage": "https://github.com/SerwisantOnline/serwisant-api-php-sdk",
  "license": "Apache-2.0",
  "require": {
    "guzzlehttp/guzzle": "^6.5",
    "php": ">=7.2",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-mbstring": "*"
  },
  "suggest": {
    "ext-pdo": "It's required only, if you decide to use AccessTokenContainerSqlite or AccessTokenContainerPDO",
    "ext-openssl": "It's required only, if you decide to use AccessTokenContainerEncryptedFile"
  },
  "authors": [
    {
      "name": "Arkadiusz Kury≈Çowicz",
      "email": "arkadiusz.kurylowicz@serwisant-online.pl"
    }
  ],
  "autoload": {
    "psr-0": {
      "Serwisant\\SerwisantApi": "src"
    }
  }
}
